{
  "name": "chapa",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "dev": "pnpm --filter @chapa/web dev",
    "build": "pnpm --filter @chapa/web build",
    "start": "pnpm --filter @chapa/web start",
    "lint": "pnpm --filter @chapa/web lint",
    "typecheck": "pnpm -r run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "pnpm --filter @chapa/web test:e2e",
    "prepare": "husky"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.0",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "vitest": "^4.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
